{
  "compilerOptions": {
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "target": "es2017",
    "module": "es6",
    "lib": [ "es2018", "es2019" ],
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "outDir": "dist_index",
    "declaration": true
  },
  "exclude": [
    "node_modules",
    "example"
  ]
}
